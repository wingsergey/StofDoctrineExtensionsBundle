{
    "name": "fintem/stof-doctrine-extensions-bundle",
    "type": "symfony-bundle",
    "description": "Integration of the gedmo/doctrine-extensions with Symfony",
    "keywords": ["tree", "behaviors", "doctrine2", "extensions", "gedmo", "sluggable","loggable", "translatable", "nestedset", "sortable", "timestampable"],
    "homepage": "https://github.com/fintem/StofDoctrineExtensionsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomas Kmieliauskas",
            "email": "tomas.kmieliauskas@fintem.lt"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.1",
        "gedmo/doctrine-extensions": "^3.0@dev"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "to use the ORM extensions",
        "doctrine/mongodb-odm-bundle": "to use the MongoDB ODM extensions"
    },
    "autoload": {
        "psr-4": { "Stof\\DoctrineExtensionsBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
