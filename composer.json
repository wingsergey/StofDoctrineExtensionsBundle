{
    "name": "wingsergey/stof-doctrine-extensions-bundle",
    "type": "symfony-bundle",
    "description": "Integration of the gedmo/doctrine-extensions with Symfony",
    "keywords": [
        "tree",
        "behaviors",
        "doctrine2",
        "extensions",
        "gedmo",
        "sluggable",
        "loggable",
        "translatable",
        "nestedset",
        "sortable",
        "timestampable"
    ],
    "homepage": "https://github.com/stof/StofDoctrineExtensionsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Coevoet",
            "email": "stof@notk.org"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "symfony/framework-bundle": "^4.3 || ^5.0",
        "gedmo/doctrine-extensions": "v3.0.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.4 || ^5.0",
        "symfony/security-bundle": "^4.3 || ^5.0"
    },
    "repositories" : [
        {
            "type": "vcs",
            "url": "git@github.com:wingsergey/DoctrineExtensions.git"
        }
    ],
    "suggest": {
        "doctrine/doctrine-bundle": "to use the ORM extensions",
        "doctrine/mongodb-odm-bundle": "to use the MongoDB ODM extensions",
        "symfony/mime": "To use the Mime component integration for Uploadable"
    },
    "autoload": {
        "psr-4": {
            "Stof\\DoctrineExtensionsBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
